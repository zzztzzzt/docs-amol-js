{
    "compilerOptions": {
      "noEmit": true,
      "target": "ESNext",
      "lib": ["ESNext", "DOM"],
      "moduleResolution": "bundler",
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "isolatedModules": true,
      "skipLibCheck": true
    },
    "include": [
      "**/*.ts", 
      "**/*.vue", 
      ".vitepress/**/*.ts",
      ".vitepress/**/*.vue"
    ]
}